{
    "name": "leblanc-simon/presta-faker",
    "description": "Init a Prestashop instance with fake values",
    "minimum-stability": "stable",
    "license": "MIT",
    "version": "1.1.0",
    "type": "project",
    "authors": [
        {
            "name": "Simon Leblanc",
            "email": "contact@leblanc-simon.eu"
        }
    ],
    "autoload": {
        "psr-0": {
            "PrestaFaker\\": "src/"
        },
        "files": ["vendor/prestashop/webservice-lib/PSWebServiceLibrary.php"]
    },
    "repositories": {
        "prestashop-webservice": {
            "type": "package",
            "package": {
                "name": "prestashop/webservice-lib",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/PrestaShop/PrestaShop-webservice-lib.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    },
    "require": {
        "fzaninotto/faker": "~1.5",
        "prestashop/webservice-lib": "1.0.0",
        "symfony/event-dispatcher": "~2.7",
        "monolog/monolog": "~1",
        "symfony/finder": "~2.7"
    }
}
